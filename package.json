{
    "name": "reader",
    "displayName": "epub reader",
    "description": "read webview",
    "version": "0.0.1",
    "publisher": "renkun",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.reader"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.reader",
                "title": "reader",
                "icon": "icon.png"
            }
        ],
        "keybindings": [{
            "command": "extension.reader",
            "key": "ctrl+d",
            "mac": "cmd+d"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
	"dependencies": {
		"vscode": "^1.1.18"
	},
	"devDependencies": {
        "vsce": "^1.53.2",
		"@types/node": "^10.5.2",
		"tslint": "^5.11.0",
		"typescript": "^2.9.2"
	}
}
